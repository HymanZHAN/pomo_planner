{
  "name": "pomo-planner",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve --hmr -o",
    "build": "ng build",
    "build:sourcemap": "ng build --source-map --named-chunks",
    "analyze": "npm run build:sourcemap && source-map-explorer dist/**/*.js --gzip --html result.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-watch": "ng test --watch",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "fmt": "prettier -w ./src --cache",
    "lint": "prettier -c ./src --cache && ng lint",
    "gfc": "ng g c --inline-style",
    "guic": "ng g c --inline-style --inline-template --flat"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngneat/elf": "^2.2.0",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-devtools": "^1.2.1",
    "@ngneat/elf-entities": "^4.4.2",
    "@tauri-apps/api": "^1.2.0",
    "@tiptap/core": "2.0.0-beta.202",
    "@tiptap/extension-bubble-menu": "2.0.0-beta.202",
    "@tiptap/extension-document": "2.0.0-beta.202",
    "@tiptap/extension-floating-menu": "2.0.0-beta.202",
    "@tiptap/extension-heading": "2.0.0-beta.202",
    "@tiptap/extension-placeholder": "2.0.0-beta.202",
    "@tiptap/extension-text": "2.0.0-beta.202",
    "@tiptap/html": "2.0.0-beta.202",
    "@tiptap/starter-kit": "2.0.0-beta.202",
    "date-fns": "^2.29.3",
    "dexie": "^3.2.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@ngneat/elf-cli": "^3.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.1",
    "@tauri-apps/cli": "^1.2.3",
    "@types/jasmine": "~4.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "eslint-plugin-rxjs-angular": "^2.0.0",
    "husky": "^8.0.2",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.8.4"
  },
  "lint-staged": {
    "*.{ts,css,scss}": [
      "prettier -c"
    ],
    "*.{ts,html}": [
      "eslint"
    ]
  }
}